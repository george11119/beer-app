<h2>List of ratings</h2>
<ul>
  <p>Number of ratings: <%= @ratings.count %></p>
  <% @ratings.each do |rating| %>
    <li>
      <%= rating %>
      <%= link_to rating.user.username, user_path(rating.user.id) %>
    </li>
  <% end %>
  <p>
    <%= link_to "New rating", new_rating_url %>
  </p>
</ul>
