<h2>List of ratings</h2>
<ul>
  <% @ratings.each do |rating| %>
    <li>
      <%= rating %> <%= button_to "delete", rating_path(rating.id), method: :delete, form: { data: { turbo_confirm: "Are you sure?" } } %>
    </li>
  <% end %>
  <p>
    <%= link_to "New rating", new_rating_url %>
  </p>
</ul>